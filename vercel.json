{
    "version": 2,
    "functions": {
        "api/**/*.py": {
            "runtime": "@vercel/python@3.1.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9",
        "PYTHONPATH": "."
    }
}